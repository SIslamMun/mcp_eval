    {
      "id": 6,
      "title": "Recent Evaluation Sessions Log",
      "type": "table",
      "gridPos": {
        "h": 12,
        "w": 24,
        "x": 0,
        "y": 24
      },
      "targets": [
        {
          "datasource": {
            "type": "influxdb",
            "uid": "dewvtownqdr0ge"
          },
          "query": "from(bucket: \"evaluation-sessions\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"evaluation_session\")\n  |> filter(fn: (r) => r[\"_field\"] == \"success\" or r[\"_field\"] == \"execution_time\" or r[\"_field\"] == \"cost_usd\")\n  |> pivot(rowKey: [\"_time\"], columnKey: [\"_field\"], valueColumn: \"_value\")\n  |> sort(columns: [\"_time\"], desc: true)\n  |> limit(n: 20)\n  |> map(fn: (r) => ({\n      _time: r._time,\n      \"Agent\": r.agent_type,\n      \"Prompt\": \"Prompt \" + string(v: r.prompt_id),\n      \"Session ID\": r.session_id,\n      \"Success\": if exists r.success then (if r.success then \"✅\" else \"❌\") else \"❓\",\n      \"Duration (s)\": if exists r.execution_time then string(v: r.execution_time) else \"N/A\",\n      \"Cost (USD)\": if exists r.cost_usd then \"$\" + string(v: r.cost_usd) else \"Free\",\n      \"Status\": r.status\n    }))",
          "refId": "A"
        }
