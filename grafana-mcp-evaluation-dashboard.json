      },
      "targets": [
        {
          "datasource": {
            "type": "influxdb",
            "uid": "dewvtownqdr0ge"
          },
          "query": "success = from(bucket: \"evaluation-sessions\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"evaluation_session\")\n  |> filter(fn: (r) => r[\"_field\"] == \"success\")\n  |> filter(fn: (r) => r[\"_value\"] == true)\n  |> group(columns: [\"agent_type\"])\n  |> count()\n\ntotal = from(bucket: \"evaluation-sessions\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"evaluation_session\")\n  |> filter(fn: (r) => r[\"_field\"] == \"success\")\n  |> group(columns: [\"agent_type\"])\n  |> count()\n\njoin(tables: {success: success, total: total}, on: [\"agent_type\"])\n  |> map(fn: (r) => ({r with _value: float(v: r._value_success) / float(v: r._value_total) * 100.0}))\n  |> group(columns: [\"agent_type\"])\n  |> last()",
          "refId": "A"
