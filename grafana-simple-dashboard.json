    "panels": [
      {
        "id": 1,
        "title": "Total Sessions",
        "type": "stat",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 0,
          "y": 0
        },
        "targets": [
          {
            "datasource": {
              "type": "influxdb",
              "uid": "dewvtownqdr0ge"
            },
            "query": "from(bucket: \"evaluation-sessions\")\n  |> range(start: -30d)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"evaluation_session\")\n  |> count()",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {
                  "color": "green",
                  "value": null
                }
              ]
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 2,
        "title": "Sessions by Agent",
        "type": "piechart",
        "gridPos": {
          "h": 8,
          "w": 6,
          "x": 6,
          "y": 0
        },
        "targets": [
          {
            "datasource": {
              "type": "influxdb",
              "uid": "dewvtownqdr0ge"
            },
            "query": "from(bucket: \"evaluation-sessions\")\n  |> range(start: -30d)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"evaluation_session\")\n  |> group(columns: [\"agent_type\"])\n  |> count()",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 3,
        "title": "Performance Metrics Over Time",
        "type": "timeseries",
        "gridPos": {
          "h": 8,
          "w": 12,
          "x": 12,
          "y": 0
        },
        "targets": [
          {
            "datasource": {
              "type": "influxdb",
              "uid": "dewvtownqdr0ge"
            },
            "query": "from(bucket: \"evaluation-sessions\")\n  |> range(start: -30d)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"performance_metrics\")\n  |> filter(fn: (r) => r[\"metric_name\"] == \"cost_usd\" or r[\"metric_name\"] == \"duration_seconds\")",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "palette-classic"
            },
            "custom": {
              "axisPlacement": "auto",
              "displayMode": "list",
              "drawStyle": "line",
              "fillOpacity": 10,
              "lineWidth": 1,
              "pointSize": 5,
              "showPoints": "auto"
            },
            "unit": "short"
          }
        }
      },
      {
        "id": 4,
        "title": "Recent Sessions",
        "type": "table",
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 8
        },
        "targets": [
          {
            "datasource": {
              "type": "influxdb",
              "uid": "dewvtownqdr0ge"
            },
            "query": "from(bucket: \"evaluation-sessions\")\n  |> range(start: -7d)\n  |> filter(fn: (r) => r[\"_measurement\"] == \"evaluation_session\")\n  |> sort(columns: [\"_time\"], desc: true)\n  |> limit(n: 10)",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
